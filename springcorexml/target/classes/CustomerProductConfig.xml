<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.CustomerBean"
		name="customer1">
		<property name="id" value="1" />
		<property name="name" value="Karan" />
		<property name="products">
			<list>
				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product1">
					<property name="id" value="1" />
					<property name="name" value="Shirts" />
				</bean>
				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product2">
					<property name="id" value="2" />
					<property name="name" value="Jeans" />
				</bean>
			</list>
		</property>
	</bean>
	<bean class="com.jspiders.springcorexml.bean.CustomerBean"
		name="customer2">
		<property name="id" value="2" />
		<property name="name" value="Arjun" />
		<property name="products">
			<list>
				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product1">
					<property name="id" value="1" />
					<property name="name" value="Shirts" />
				</bean>
				<bean class="com.jspiders.springcorexml.bean.ProductBean"
					name="product2">
					<property name="id" value="2" />
					<property name="name" value="Jeans" />
				</bean>
			</list>
		</property>
	</bean>



</beans>